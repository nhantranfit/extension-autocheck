{
    "manifest_version": 2,
    "name": "Sinh Vien",
    "description": "Sinh Vien",
    "version": "1.0",
    "permissions": ["tabs", "*://*/*", "storage"],
    "content_scripts": [{
        "matches": [
            "<all_urls>"
        ],
        "js": ["public/js/jquery.js" , "public/js/popup.js"]
    }],
    "browser_action": {
        "default_title": "Sinh Vien",
        "default_popup": "popup.html",
        "default_icon": "public/image/user_profile.png"
    }
}